    "dependencies": {
        "gpii-express": "1.0.15",
        "infusion": "3.0.0-dev.20191220T163226Z.db83ce0ef.FLUID-6145"
    },
    "devDependencies": {
        "eslint": "6.8.0",
        "eslint-config-fluid": "1.4.0",
        "gpii-binder": "1.1.0",
        "gpii-grunt-lint-all": "1.0.7",
        "gpii-testem": "2.1.11",
        "gpii-webdriver": "1.1.0",
        "grunt": "1.0.4",
        "kettle": "1.11.0",
        "node-jqunit": "1.1.8",
        "nyc": "14.1.1"
    }
